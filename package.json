{
	"name": "crowdin-test",
	"displayName": "Crowdin-test",
	"description": "Preview available projects with there details in Crowdin-based projects",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:crowdin-test.helloWorld",
		"onCommand:crowdin-test.askQuestion",
		"onCommand:crowdin-test.refresh",
		"onView:crowdin-test-sidebar"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "crowdin-test.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "crowdin-test.askQuestion",
				"category": "Crowdin test",
				"title": "Ask question"
			},
			{
				"command": "crowdin-test.refresh",
				"category": "Crowdin test",
				"title": "Refresh"
			}
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "crowdin-test-sidebar-view",
					"title": "Crowdin Test",
					"icon": "media/ww.svg"
				}
			]
		},
		"views": {
			"crowdin-test-sidebar-view": [
				{
					"type": "webview",
					"id": "crowdin-test-sidebar",
					"name": "Crowdin Test",
					"icon": "media/ww.svg",
					"visibility": "visible",
					"contextualTitle": "Crwodin Test"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "rollup -c && webpack --mode production",
		"watch": "concurrently \"rollup -c -w\" \"webpack --mode development --watch\"",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^19.0.2",
		"@rollup/plugin-node-resolve": "^13.0.4",
		"@rollup/plugin-typescript": "^8.2.3",
		"@tsconfig/svelte": "^2.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"concurrently": "^6.2.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"rollup": "^2.55.1",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.41.0",
		"svelte-check": "^2.2.3",
		"svelte-preprocess": "^4.7.4",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"dependencies": {
		"@crowdin/crowdin-api-client": "^1.11.3"
	}
}
